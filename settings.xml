<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <proxies>
    <proxy>
      <id>fujitsu-http-proxy</id>
      <active>true</active>
      <protocol>http</protocol>
      <host>proxy.intern.est.fujitsu.com</host>
      <port>8080</port>
      <nonProxyHosts>estessppbuild1|*.intern.est.fujitsu.com</nonProxyHosts>
    </proxy>
    <proxy>
      <id>fujitsu-https-proxy</id>
      <active>true</active>
      <protocol>https</protocol>
      <host>proxy.intern.est.fujitsu.com</host>
      <port>8080</port>
      <nonProxyHosts>estessppbuild1|*.intern.est.fujitsu.com</nonProxyHosts>
    </proxy>
  </proxies>

</settings>